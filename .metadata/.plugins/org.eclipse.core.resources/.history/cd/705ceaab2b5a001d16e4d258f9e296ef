package main.act1_20;

public class Mas25 extends TiempoEjecucion implements Runnable {
	Alumno[] alumnos;
	double numAlumnos;
	
	public Mas25(Alumno[] alumnos) {
		this.alumnos = alumnos;
	}

	@Override
	public void run() {
		for(Alumno x:this.alumnos) {
			if(x.getEdad() > 25) {
				this.numAlumnos++;
			}
		}
	}

	public double getNumAlumnos() {
		return numAlumnos;
	}
}
