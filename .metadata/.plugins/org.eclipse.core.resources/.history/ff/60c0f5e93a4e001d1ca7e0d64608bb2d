package main.act1_12;
/**
 * @author lucia
 * @version 1.0
 */
public class Act1_12_dame {
	// El número de veces que se debe repetir la cadena
	private static final int NUM_REP = 5;
	
	public static void main(String[] args) {
		// Se asegura de que haya una cadena, y además String:
		if(args.length > 0) {
			for(int i = 0; i < NUM_REP; i++) {
				System.out.println(args[0]);
			}
		System.exit(0);
		// Si no se ha pasado cadena:
		} else {
			System.exit(-1);
		}
	}
}
