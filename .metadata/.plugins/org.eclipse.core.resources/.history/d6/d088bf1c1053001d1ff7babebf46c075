package main.act1_15;

public class Main extends Thread {
	
	public static void main(String[] args) {
		Hilo ping = new Hilo("PING");
		Hilo pong = new Hilo("PONG");

		ping.getHilo().start();
		try {
			ping.getHilo().join();
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		pong.getHilo().start();

	}
}