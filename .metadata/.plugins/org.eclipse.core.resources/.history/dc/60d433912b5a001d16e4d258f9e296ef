package main.act1_20;

public class De19Anios implements Runnable {
	Alumno[] alumnos;
	double numAlumnos;
	private long tiempoEjecucion = 0;
	
	public De19Anios(Alumno[] alumnos) {
		this.alumnos = alumnos;
	}

	@Override
	public void run() {
		for(Alumno x:this.alumnos) {
			if(x.getEdad() == 19) {
				this.numAlumnos++;
			}
		}
	}

	public double getNumAlumnos() {
		return numAlumnos;
	}
}